class Shape:#protected variables
    _length = 10 
    _breadth = 20
    def __init__():
        print(' For Class Shape  - object is created!!')
    
    def _area(self):
        print('Protected method is called')


class Circle(Shape):  # derived class from shape class
    def __init__(self):#printing protected variables in the derived class
        print(self._length)
        print(self._breadth)
        self._area()

cr = Circle()
